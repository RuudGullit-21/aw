		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	уатДокументыВодителей.ВладелецДокументов КАК Объект,
	уатДокументыВодителей.ВидДокумента КАК ВидДокумента,
	уатДокументыВодителей.Серия КАК Серия,
	уатДокументыВодителей.Номер КАК Номер,
	уатДокументыВодителей.ДатаВыдачи КАК ДатаВыдачи,
	уатДокументыВодителей.ДатаОкончания КАК ДатаОкончания,
	уатДокументыВодителей.Статус КАК Статус,
	уатДокументыВодителей.КемВыдан КАК КемВыдан,
	уатДокументыВодителей.ОткрытыеКатегории КАК ОткрытыеКатегории,
	1 КАК Количество,
	ВЫБОР
		КОГДА РАЗНОСТЬДАТ(&amp;Период, уатДокументыВодителей.ДатаОкончания, ДЕНЬ) = 0
			ТОГДА 0
		КОГДА уатДокументыВодителей.ДатаОкончания = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;Период, уатДокументыВодителей.ДатаОкончания, ДЕНЬ)
	КОНЕЦ КАК ОсталосьДней,
	ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументВодителя) КАК ПринадлежностьДокумента,
	уатДокументыВодителей.Ссылка КАК ДокументСсылка,
	NULL КАК КемВыданаТК,
	уатДокументыВодителей.Организация КАК Организация,
	уатДокументыВодителей.Подразделение КАК Подразделение
ПОМЕСТИТЬ втДокументы
ИЗ
	Справочник.уатРегистрационныеДокументы КАК уатДокументыВодителей
ГДЕ
	(уатДокументыВодителей.ВладелецДокументов.ДатаУвольнения = ДАТАВРЕМЯ(1, 1, 1) ИЛИ уатДокументыВодителей.ВладелецДокументов.ДатаУвольнения &gt; &amp;Период)
	И уатДокументыВодителей.ПринадлежностьДокументов = ЗНАЧЕНИЕ(Перечисление.уатПринадлежностьДокументов.ДокументыВодителя)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	уатДокументыТС.ВладелецДокументов,
	уатДокументыТС.ВидДокумента,
	уатДокументыТС.Серия,
	уатДокументыТС.Номер,
	уатДокументыТС.ДатаВыдачи,
	уатДокументыТС.ДатаОкончания,
	уатДокументыТС.Статус,
	уатДокументыТС.КемВыдан,
	уатДокументыТС.ОткрытыеКатегории,
	1,
	ВЫБОР
		КОГДА РАЗНОСТЬДАТ(&amp;Период, уатДокументыТС.ДатаОкончания, ДЕНЬ) = 0
			ТОГДА 0
		КОГДА уатДокументыТС.ДатаОкончания = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;Период, уатДокументыТС.ДатаОкончания, ДЕНЬ)
	КОНЕЦ,
	ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументТС),
	уатДокументыТС.Ссылка,
	NULL,
	уатДокументыТС.Организация,
	уатДокументыТС.Подразделение
ИЗ
	Справочник.уатРегистрационныеДокументы КАК уатДокументыТС
ГДЕ
	уатДокументыТС.ПринадлежностьДокументов = ЗНАЧЕНИЕ(Перечисление.уатПринадлежностьДокументов.ДокументыТС)
	И (уатДокументыТС.ВладелецДокументов.ДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1)
			ИЛИ уатДокументыТС.ВладелецДокументов.ДатаВыбытия &gt; &amp;Период)

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	уатПластиковыеКарты.КомуВыдана,
	"Топливная карта",
	"",
	уатПластиковыеКарты.Код,
	уатПластиковыеКарты.ДатаВыдачи,
	уатПластиковыеКарты.ДатаОкончания,
	уатПластиковыеКарты.Статус,
	NULL,
	"",
	1,
	ВЫБОР
		КОГДА РАЗНОСТЬДАТ(&amp;Период, уатПластиковыеКарты.ДатаОкончания, ДЕНЬ) = 0
			ТОГДА 0
		КОГДА уатПластиковыеКарты.ДатаОкончания = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА 0
		ИНАЧЕ РАЗНОСТЬДАТ(&amp;Период, уатПластиковыеКарты.ДатаОкончания, ДЕНЬ)
	КОНЕЦ,
	ВЫБОР
		КОГДА ТИПЗНАЧЕНИЯ(уатПластиковыеКарты.КомуВыдана) = ТИП(Справочник.уатТС)
			ТОГДА ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументТС)
		ИНАЧЕ ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументВодителя)
	КОНЕЦ,
	уатПластиковыеКарты.Ссылка,
	уатПластиковыеКарты.КемВыдана,
	NULL,
	NULL
ИЗ
	Справочник.уатПластиковыеКарты КАК уатПластиковыеКарты
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
	втДокументы.ВидДокумента КАК ВидДокумента,
	втДокументы.Серия КАК Серия,
	втДокументы.Номер КАК Номер,
	втДокументы.ДатаВыдачи КАК ДатаВыдачи,
	втДокументы.ДатаОкончания КАК ДатаОкончания,
	втДокументы.Статус КАК Статус,
	втДокументы.КемВыдан КАК КемВыдан,
	втДокументы.ОткрытыеКатегории КАК ОткрытыеКатегории,
	втДокументы.Количество КАК Количество,
	втДокументы.ОсталосьДней КАК ОсталосьДней,
	втДокументы.ПринадлежностьДокумента КАК ПринадлежностьДокумента,
	втДокументы.ДокументСсылка КАК Ссылка,
	втДокументы.Объект КАК Объект,
	втДокументы.КемВыданаТК КАК КемВыданаТК,
	ВЫБОР
		КОГДА втДокументы.ПринадлежностьДокумента = ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументВодителя)
			ТОГДА ЕСТЬNULL(втДокументы.Подразделение, уатКадроваяИсторияСотрудниковСрезПоследних.Подразделение)
		ИНАЧЕ ЕСТЬNULL(втДокументы.Подразделение, уатМестонахождениеТССрезПоследних.Подразделение)
	КОНЕЦ КАК Подразделение,
	ВЫБОР
		КОГДА втДокументы.ПринадлежностьДокумента = ЗНАЧЕНИЕ(Перечисление.уатПолеОтчетаВидДокументаСотрудникТС.ДокументВодителя)
			ТОГДА ЕСТЬNULL(втДокументы.Организация, уатКадроваяИсторияСотрудниковСрезПоследних.Организация)
		ИНАЧЕ ЕСТЬNULL(втДокументы.Организация, уатМестонахождениеТССрезПоследних.Организация)
	КОНЕЦ КАК Организация
ИЗ
	втДокументы КАК втДокументы
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатМестонахождениеТС.СрезПоследних(&amp;Период, ) КАК уатМестонахождениеТССрезПоследних
		ПО втДокументы.Объект = уатМестонахождениеТССрезПоследних.ТС		
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрСведений.уатКадроваяИсторияСотрудников.СрезПоследних(&amp;Период, ) КАК уатКадроваяИсторияСотрудниковСрезПоследних
		ПО втДокументы.Объект = уатКадроваяИсторияСотрудниковСрезПоследних.Сотрудник</query>
