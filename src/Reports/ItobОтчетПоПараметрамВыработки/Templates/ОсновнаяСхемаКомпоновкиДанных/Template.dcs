	<totalField>
		<dataPath>ЗначениеПараметра</dataPath>
		<expression>ВЫБОР 
	КОГДА ТипИтога="СРЕДНЕЕ" ТОГДА 
		ВЫБОР
		    КОГДА СРЕДНЕЕ(ЗначениеПараметра) ЕСТЬ NULL ТОГДА "-"
			КОГДА ЕдИзм="чч:мм" ТОГДА
				ВЫБОР 
					КОГДА ВЫРАЗИТЬ(СРЕДНЕЕ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ(СРЕДНЕЕ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ(СРЕДНЕЕ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ + ":" +
				ВЫБОР
				    КОГДА ВЫРАЗИТЬ((СРЕДНЕЕ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ((СРЕДНЕЕ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ((СРЕДНЕЕ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ
			КОГДА ЕдИзм="км/ч" ИЛИ ЕдИзм="л." ТОГДА ФОРМАТ(СРЕДНЕЕ(ЗначениеПараметра),"ЧДЦ=; ЧН=0")
			ИНАЧЕ ФОРМАТ(СРЕДНЕЕ(ЗначениеПараметра),"ЧДЦ=1; ЧН=0")
		КОНЕЦ		
	КОГДА ТипИтога="МАКСИМУМ" ТОГДА 
		ВЫБОР
		    КОГДА МАКСИМУМ(ЗначениеПараметра) ЕСТЬ NULL ТОГДА "-"
			КОГДА ЕдИзм="чч:мм" ТОГДА			    
				ВЫБОР 
					КОГДА ВЫРАЗИТЬ(МАКСИМУМ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ(МАКСИМУМ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ(МАКСИМУМ(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ + ":" +
				ВЫБОР
				    КОГДА ВЫРАЗИТЬ((МАКСИМУМ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ((МАКСИМУМ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ((МАКСИМУМ(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ
			КОГДА ЕдИзм="км/ч" ИЛИ ЕдИзм="л." ТОГДА ФОРМАТ(МАКСИМУМ(ЗначениеПараметра),"ЧДЦ=; ЧН=0")
			ИНАЧЕ ФОРМАТ(МАКСИМУМ(ЗначениеПараметра),"ЧДЦ=1; ЧН=0")		
		КОНЕЦ
	ИНАЧЕ  
		ВЫБОР
			КОГДА СУММА(ЗначениеПараметра) ЕСТЬ NULL ТОГДА "-"
			КОГДА ЕдИзм="чч:мм" ТОГДА
				ВЫБОР 
					КОГДА ВЫРАЗИТЬ(СУММА(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ(СУММА(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ(СУММА(ЗначениеПараметра)-0.4999999999,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ + ":" +
				ВЫБОР
				    КОГДА ВЫРАЗИТЬ((СУММА(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)") &lt; 10 
						ТОГДА "0" + Формат(ВЫРАЗИТЬ((СУММА(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=0; ЧГ=0") 
					ИНАЧЕ Формат(ВЫРАЗИТЬ((СУММА(ЗначениеПараметра*3600)%3600)/60,"ЧИСЛО(15,0)"),"ЧДЦ=; ЧН=00; ЧГ=0")
				КОНЕЦ
			КОГДА ЕдИзм="км/ч" ИЛИ ЕдИзм="л." ТОГДА ФОРМАТ(СУММА(ЗначениеПараметра),"ЧДЦ=; ЧН=0")	
			ИНАЧЕ ФОРМАТ(СУММА(ЗначениеПараметра),"ЧДЦ=1; ЧН=0")		
		КОНЕЦ
