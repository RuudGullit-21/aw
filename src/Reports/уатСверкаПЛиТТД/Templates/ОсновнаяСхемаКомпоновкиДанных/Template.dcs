		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
    уатТТДВыработка.Ссылка КАК ТТД,
    уатТТДВыработка.ПутЛист КАК ПутЛист,
    уатТТДВыработка.Ссылка.ВалютаДокумента КАК ВалютаТТД
ПОМЕСТИТЬ втВыработка
ИЗ
    Документ.уатТТД.Выработка КАК уатТТДВыработка
ГДЕ
    уатТТДВыработка.Ссылка.Проведен

СГРУППИРОВАТЬ ПО
    уатТТДВыработка.ПутЛист,
    уатТТДВыработка.Ссылка.ВалютаДокумента,
    уатТТДВыработка.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
    СУММА(ЕСТЬNULL(уатПредоставленныеУслуги.СуммаРегл, 0)) КАК Сумма,
    СУММА(ЕСТЬNULL(уатПредоставленныеУслуги.Сумма, 0)) КАК СуммаВзаим,
    втВыработка.ТТД КАК ТТД,
    втВыработка.ПутЛист КАК ПутЛист,
    втВыработка.ВалютаТТД КАК ВалютаТТД
ПОМЕСТИТЬ втУслуги
ИЗ
    втВыработка КАК втВыработка
        ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.уатПредоставленныеУслуги КАК уатПредоставленныеУслуги
        ПО втВыработка.ТТД = уатПредоставленныеУслуги.Регистратор
            И втВыработка.ПутЛист = уатПредоставленныеУслуги.ПутевойЛист

СГРУППИРОВАТЬ ПО
    втВыработка.ТТД,
    втВыработка.ПутЛист,
    втВыработка.ВалютаТТД
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ РАЗРЕШЕННЫЕ
    уатПутевойЛист.Ссылка КАК ПутевойЛист,
    уатПутевойЛист.Организация КАК Организация,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, НЕДЕЛЯ) КАК ПериодНеделя,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, ДЕНЬ) КАК ПериодДень,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, ДЕКАДА) КАК ПериодДекада,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, МЕСЯЦ) КАК ПериодМесяц,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, КВАРТАЛ) КАК ПериодКвартал,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, ПОЛУГОДИЕ) КАК ПериодПолугодие,
    НАЧАЛОПЕРИОДА(уатПутевойЛист.Ссылка.ДатаЗакрытия, ГОД) КАК ПериодГод,
    уатПутевойЛист.Ссылка.ТранспортноеСредство КАК ТС,
    втУслуги.ТТД КАК ТТД,
    втУслуги.ВалютаТТД КАК ВалютаТТД,
    ЕСТЬNULL(втУслуги.Сумма, 0) КАК Сумма,
    ЕСТЬNULL(втУслуги.СуммаВзаим, 0) КАК СуммаВзаим
ИЗ
    Документ.уатПутевойЛист КАК уатПутевойЛист
        ЛЕВОЕ СОЕДИНЕНИЕ втУслуги КАК втУслуги
        ПО уатПутевойЛист.Ссылка = втУслуги.ПутЛист
ГДЕ
    уатПутевойЛист.Ссылка.Проведен
    И уатПутевойЛист.Ссылка.Рассчитан
    И уатПутевойЛист.ДатаЗакрытия МЕЖДУ &amp;ДатаНачала И &amp;ДатаКонца
    И ВЫБОР
            КОГДА &amp;СкрытьСформированныеТТД
                ТОГДА ВЫБОР
                        КОГДА втУслуги.ТТД ЕСТЬ NULL
                            ТОГДА ИСТИНА
                        ИНАЧЕ ЛОЖЬ
                    КОНЕЦ
            ИНАЧЕ ИСТИНА
        КОНЕЦ</query>
	</dataSet>
