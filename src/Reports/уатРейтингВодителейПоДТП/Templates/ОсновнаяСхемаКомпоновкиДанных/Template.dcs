		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
    уатДТП.Водитель КАК Водитель,
    уатДТП.ТС КАК ТС,
    уатДТП.ПричинаДТП КАК ПричинаДТП,
    уатДТП.Виновность КАК Виновность,
    уатДТП.Организация КАК Организация,
    уатДТП.Ссылка КАК ДТП,
    СУММА(уатДТП.СуммаРемонта) КАК СуммаРемонта,
    КОЛИЧЕСТВО(РАЗЛИЧНЫЕ уатДТП.Ссылка) КАК КоличествоДТП
ИЗ
    Документ.уатДТП КАК уатДТП
ГДЕ
    НЕ уатДТП.ПометкаУдаления
    И уатДТП.Дата &gt;= &amp;НачалоПериода
    И (&amp;КонецПериода = ДАТАВРЕМЯ(1, 1, 1)
            ИЛИ уатДТП.Дата &lt;= &amp;КонецПериода)
    И (&amp;ОтображатьВыбывшиеТС
            ИЛИ уатДТП.ТС.ДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1))

СГРУППИРОВАТЬ ПО
    уатДТП.Водитель,
    уатДТП.ТС,
    уатДТП.ПричинаДТП,
    уатДТП.Виновность,
    уатДТП.Организация,
    уатДТП.Ссылка

УПОРЯДОЧИТЬ ПО
    СуммаРемонта</query>
