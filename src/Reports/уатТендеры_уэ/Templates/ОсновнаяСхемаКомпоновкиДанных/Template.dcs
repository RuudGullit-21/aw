			<field>СтатусТендера</field>
		</field>
		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
    уатТендер_уэПеревозчики.Ссылка КАК Тендер,
    уатТендер_уэПеревозчики.Перевозчик КАК Перевозчик,
    уатТендер_уэПеревозчики.ДатаОтвета КАК ДатаОтвета,
    уатТендер_уэПеревозчики.ЗапросОтправлен КАК ЗапросОтправлен,
    уатТендер_уэПеревозчики.Комментарий КАК Комментарий,
    уатТендер_уэПеревозчики.Ответ КАК Ответ,
    уатТендер_уэПеревозчики.СпособЗапроса КАК СпособЗапроса,
    уатТендер_уэПеревозчики.ЭтоПобедитель КАК ЭтоПобедитель,
    ВЫБОР
        КОГДА уатТендер_уэ.УчитыватьНДС
                И уатТендер_уэ.СуммаВключаетНДС
            ТОГДА уатТендер_уэПеревозчики.Сумма - уатТендер_уэПеревозчики.СуммаНДС
        ИНАЧЕ уатТендер_уэПеревозчики.Сумма
    КОНЕЦ КАК СуммаБезНДС,
    ВЫБОР
        КОГДА уатТендер_уэ.УчитыватьНДС
                И уатТендер_уэ.СуммаВключаетНДС
            ТОГДА уатТендер_уэПеревозчики.Сумма
        ИНАЧЕ уатТендер_уэПеревозчики.Сумма + уатТендер_уэПеревозчики.СуммаНДС
    КОНЕЦ КАК СуммаСНДС,
    уатТендер_уэ.СтатусТендера КАК СтатусТендера
ИЗ
    Документ.уатТендер_уэ.Перевозчики КАК уатТендер_уэПеревозчики
        ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.уатТендер_уэ КАК уатТендер_уэ
        ПО уатТендер_уэПеревозчики.Ссылка = уатТендер_уэ.Ссылка
ГДЕ
    уатТендер_уэ.Дата МЕЖДУ &amp;ДатаНачала И &amp;ДатаОкончания</query>
