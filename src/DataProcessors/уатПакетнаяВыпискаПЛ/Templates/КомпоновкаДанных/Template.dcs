                    уатПутевойЛистЗадание.Ссылка.ПометкаУдаления = ЛОЖЬ
                    И уатПутевойЛистЗадание.Ссылка.ДатаВыезда МЕЖДУ &amp;ДатаНач И &amp;ДатаКон
                    И уатПутевойЛистЗадание.Ссылка.Организация = &amp;Организация
                    И уатПутевойЛистЗадание.НомерСтроки = 2) КАК ВтораяСтрока
                ПО уатПутевойЛист.Ссылка = ВтораяСтрока.Ссылка
        ГДЕ
            уатПутевойЛист.ПометкаУдаления = ЛОЖЬ
            И уатПутевойЛист.Организация = &amp;Организация
            И уатПутевойЛист.ДатаВыезда МЕЖДУ &amp;ДатаНач И &amp;ДатаКон) КАК ПутевыеЛисты
        ПО уатТС.Ссылка = ПутевыеЛисты.ТС
ГДЕ
    &amp;ТипЗаполнения = 0
    И уатМестонахождениеТССрезПоследних.Организация = &amp;Организация
    И уатТС.Модель.ВидМоделиТС = ЗНАЧЕНИЕ(Перечисление.уатВидыМоделейТС.Автотранспорт)
    И (уатТС.ТипТС = ЗНАЧЕНИЕ(Справочник.уатТипыТС.ПустаяСсылка)
            ИЛИ НЕ уатТС.ТипТС.ВидТС В (&amp;СписокПрицепов))
    И (уатТС.ДатаВыбытия = ДАТАВРЕМЯ(1, 1, 1)
            ИЛИ уатТС.ДатаВыбытия &gt; &amp;ДатаКон)
    И НЕ уатТС.ПометкаУдаления
    И (&amp;Колонна = НЕОПРЕДЕЛЕНО
            ИЛИ уатМестонахождениеТССрезПоследних.Колонна В ИЕРАРХИИ (&amp;Колонна))
    И (&amp;Подразделение = НЕОПРЕДЕЛЕНО
            ИЛИ уатМестонахождениеТССрезПоследних.Подразделение В ИЕРАРХИИ (&amp;Подразделение))
    И (&amp;Гараж = НЕОПРЕДЕЛЕНО
            ИЛИ уатТС.Гараж В ИЕРАРХИИ (&amp;Гараж))
    И (&amp;Модель = НЕОПРЕДЕЛЕНО
            ИЛИ уатТС.Модель В ИЕРАРХИИ (&amp;Модель))</query>
		</item>
	</dataSet>
