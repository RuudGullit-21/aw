		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	уатМаршрутныйЛист.Ссылка,
	уатМаршрутныйЛист.АдресОтправления,
	уатМаршрутныйЛист.АдресНазначения,
	уатМаршрутныйЛист.Номер,
	уатМаршрутныйЛист.Дата,
	уатМаршрутныйЛист.Водитель1,
	уатМаршрутныйЛист.ДатаИВремяОтправленияПлан,
	уатМаршрутныйЛист.ДатаИВремяОтправления,
	уатМаршрутныйЛист.ДатаИВремяВыполнения,
	уатМаршрутныйЛист.Контрагент,
	уатМаршрутныйЛист.Ответственный,
	уатМаршрутныйЛист.ВидПеревозки,
	уатМаршрутныйЛист.ТС,
	уатМаршрутныйЛист.ДоговорКонтрагента,
	уатМаршрутныйЛист.Подразделение,
	уатМаршрутныйЛист.ОбъектСтроительства,
	уатМаршрутныйЛист.Маршрут,
	ВЫБОР
		КОГДА уатМаршрутныйЛист.ДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Закрыт,
	ВЫБОР
		КОГДА уатМаршрутныйЛист.ДатаИВремяОтправления = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Отправлен,
	ВЫБОР
		КОГДА уатМаршрутныйЛист.ДатаИВремяВыполнения = ДАТАВРЕМЯ(1, 1, 1)
			ТОГДА ЛОЖЬ
		ИНАЧЕ ИСТИНА
	КОНЕЦ КАК Выполнен
ИЗ
	Документ.уатМаршрутныйЛист КАК уатМаршрутныйЛист
ГДЕ
	НЕ уатМаршрутныйЛист.ПометкаУдаления
	И уатМаршрутныйЛист.Проведен
	И уатМаршрутныйЛист.Организация = &amp;Организация
	И уатМаршрутныйЛист.Дата МЕЖДУ &amp;ДатаНач И &amp;ДатаКон</query>
