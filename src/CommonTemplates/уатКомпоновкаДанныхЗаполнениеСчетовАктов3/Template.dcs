	1,
	-уатРекламация_уэРаспределение.Сумма,
	-уатРекламация_уэРаспределение.СуммаНДС,
	уатРекламация_уэ.ЗаказГрузоотправителя,
	ЕСТЬNULL(уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления, ЗНАЧЕНИЕ(Справочник.уатПунктыНазначения.ПустаяСсылка)),
	ЕСТЬNULL(уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения, ЗНАЧЕНИЕ(Справочник.уатПунктыНазначения.ПустаяСсылка)),
	ВЫБОР
		КОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления ЕСТЬ NULL
			ТОГДА ЗНАЧЕНИЕ(Справочник.уатПунктыНазначения.ПустаяСсылка)
		ИНАЧЕ ВЫРАЗИТЬ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления КАК Справочник.уатПунктыНазначения)
	КОНЕЦ,
	ВЫБОР
		КОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения ЕСТЬ NULL
			ТОГДА ЗНАЧЕНИЕ(Справочник.уатПунктыНазначения.ПустаяСсылка)
		ИНАЧЕ ВЫРАЗИТЬ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения КАК Справочник.уатПунктыНазначения)
	КОНЕЦ,
	ВЫБОР
		КОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления ССЫЛКА Справочник.уатПунктыНазначения
			ТОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления.Наименование
		КОГДА ТИПЗНАЧЕНИЯ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления) = ТИП(СТРОКА)
			ТОГДА ВЫРАЗИТЬ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресОтправления КАК СТРОКА(500))
		ИНАЧЕ ""
	КОНЕЦ,
	ВЫБОР
		КОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения ССЫЛКА Справочник.уатПунктыНазначения
			ТОГДА уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения.Наименование
		КОГДА ТИПЗНАЧЕНИЯ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения) = ТИП(СТРОКА)
			ТОГДА ВЫРАЗИТЬ(уатРекламация_уэ.ЗаказГрузоотправителя.АдресНазначения КАК СТРОКА(500))
		ИНАЧЕ ""
	КОНЕЦ,
	НЕОПРЕДЕЛЕНО,
	уатРекламация_уэРаспределение.СтавкаНДС,
	НЕОПРЕДЕЛЕНО,
	НЕОПРЕДЕЛЕНО,
	ВЫБОР
		КОГДА &amp;ВидСортировки = 1
			ТОГДА уатРекламация_уэ.ЗаказГрузоотправителя.ОтправлениеС
		КОГДА &amp;ВидСортировки = 2
			ТОГДА уатРекламация_уэ.ЗаказГрузоотправителя.ДоставкаС
		ИНАЧЕ уатРекламация_уэ.Дата
	КОНЕЦ,
	НЕОПРЕДЕЛЕНО,
	ЛОЖЬ,
	ВЫБОР
		КОГДА уатРекламация_уэРаспределение.Валюта = ЗНАЧЕНИЕ(Справочник.Валюты.ПустаяСсылка)
			ТОГДА уатРекламация_уэ.ДоговорКонтрагента.ВалютаВзаиморасчетов
		ИНАЧЕ уатРекламация_уэРаспределение.Валюта
	КОНЕЦ,
	уатРекламация_уэ.Ссылка
ИЗ
	Документ.уатРекламация_уэ КАК уатРекламация_уэ
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.уатМаршрутныйЛист КАК уатМаршрутныйЛист
		ПО уатРекламация_уэ.МаршрутныйЛист = уатМаршрутныйЛист.Ссылка
		ЛЕВОЕ СОЕДИНЕНИЕ Документ.уатРекламация_уэ.Распределение КАК уатРекламация_уэРаспределение
		ПО уатРекламация_уэ.Ссылка = уатРекламация_уэРаспределение.Ссылка
ГДЕ
	уатРекламация_уэ.Организация = &amp;Организация
	И ВЫБОР
			КОГДА &amp;Перевозчик &lt;&gt; ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
				ТОГДА уатРекламация_уэРаспределение.Контрагент = &amp;Перевозчик
						И уатРекламация_уэРаспределение.ДоговорКонтрагента = &amp;ДоговорКонтрагента
			ИНАЧЕ уатРекламация_уэРаспределение.Контрагент &lt;&gt; &amp;Контрагент
		КОНЕЦ
	И &amp;ПоРекламациям
	И уатРекламация_уэ.Проведен = ИСТИНА
	И ВЫБОР
			КОГДА уатМаршрутныйЛист.ДатаЗакрытия = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА уатМаршрутныйЛист.Дата
			ИНАЧЕ уатМаршрутныйЛист.ДатаЗакрытия
		КОНЕЦ МЕЖДУ &amp;НачПериодаРекламации И &amp;КонПериодаРекламации

УПОРЯДОЧИТЬ ПО
	ДатаДоставки,
	ДокСортировка,
	уатПредоставленныеУслугиОбороты.Номенклатура</query>
