        playRoute(points, speed = 2000, icon = 0) { 
			
			this.waypoints = [];
			for (let i = 0; i < points.length; i++) {
				this.waypoints.push(points[i]);
			}
			
            // Выбираем иконку для роутинга
            let blankIcons = this.blankIcons(icon);

            if (this.playMarker !== null && this.playMarker.isRunning()) {
                this.map.removeLayer(this.playMarker);
                this.reporting("playStop", "0", "Stop play", "");
            }

            if (this.waypoints.length > 0) {
                this.playMarker = L.Marker.movingMarker(this.waypoints, speed, {icon: blankIcons}).addTo(this.map);
                if (speed !== 0) {
                    this.playMarker.start();
                } else {
                    this.reporting("playStop", "0", "Stop play", "");
                    this.map.removeLayer(this.playMarker);
                }
                this.playMarker.on("end", () => {
                    this.reporting("playStop", "0", "End play", "");
                    this.map.removeLayer(this.playMarker);
                })
            } else {
                this.reporting("playStop", "1", "No waypoints to play", "");
            }
        }

        setWaypointsFactToPlay(coordinate) {
            this.waypoints = coordinate;
        }

        drawPolygon(arr, redact) {
            try {
                let layer;
                for (let i = 0; i < arr.length; i++) {
                    // Описание области
                    layer = L.polygon(arr[i].latlng, {color: arr[i].color, guid: arr[i].guid});
                    // Добавляем область на карту
                    this.editableLayers.addLayer(layer);

                    if (redact && this.editable) {
                        layer.on('click', function (e) {
                            if (maps.unSelectedLayer !== e.target) {
                                // Проверяем существует ли предыдущая область
                                if (maps.unSelectedLayer == null && maps.selectedLayer != null) {
                                    // удаляем слой который не сохранили
                                    //this.map.removeLayer(maps.selectedLayer);

                                    // очищаем рабочие слои
                                    maps.unSelectedLayer = maps.selectedLayer = null;

                                    // скрываем кнопки управления
                                    maps.togglePanel(false, '#polygonCancelButton');
                                }
                                if (maps.unSelectedLayer != null) {
                                    // отключаем редактирование области
                                    maps.unSelectedLayer.disableEdit();

                                    // восстанавливаем его старые координаты
                                    maps.unSelectedLayer.setLatLngs(maps.oldLatLngs);
                                }
                                // Обновляем объект по которому произошел клик
                                maps.unSelectedLayer = maps.selectedLayer = e.target;

                                if (maps.selectedLayer != null) {
                                    maps.togglePanel(true, '#polygonCancelButton');
                                }

                                // Запоминаем старые координаты выбранной области
                                maps.oldLatLngs = maps.coordinateFormat(maps.selectedLayer).latLngs;
                                // Центрируем редактируемую область
                                // maps.setBoundsToWay(maps.selectedLayer.getBounds());
                                // Включаем режим редактирования
                                maps.selectedLayer.enableEdit();
                                // Включаем опцию, активный слой поверх остальных
                                maps.selectedLayer.bringToFront();
                                // Отправляем guid при click по области
                                maps.reporting("geozoneSelection", "0", "Geozone selection", '<guid>' + maps.selectedLayer.options.guid + '</guid>');
                            }
                        });
                        this.togglePanel(true, '.action-create');
                    }
                }

                //  после построения областей центрируем карту
                this.map.fitBounds(this.editableLayers.getBounds());
                return layer;
            } catch (e) {
				
            }
        }
    }

