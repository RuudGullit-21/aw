                        this.oldLatLngs = this.coordinateFormat(this.selectedLayer).latLngs;
                        this.selectedLayer.disableEdit();
                    }
					this.unSelectedLayer = this.selectedLayer = e;
					this.oldLatLngs = this.coordinateFormat(this.selectedLayer).latLngs;
					this.selectedLayer.enableEdit();
					this.togglePanel(true, '.action-update');
                }
            });
        }
    }

    // Событие которое говорит 1С, что все успешно загружено (такая логика на 1С)
    maps.reporting("<leafletResponse><command>scriptLoad</command><code>0</code><description>Script is load</description><data></data></leafletResponse>");
</script>

