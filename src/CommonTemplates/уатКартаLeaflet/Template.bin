    let blankIcons = null;

    // Общая функция  инициализации
    function launchMap(className, editable, zoomControl, zoom, center, scaleMetric, tileServer, osrmServer) {
        if (maps) {
            maps.map.remove();
        }
        switch (className) {
            case "Depict":
                maps = new Depict(editable, zoomControl, zoom, center, scaleMetric, tileServer, osrmServer);
                break;
            case "Geozone":
                maps = new Geozone(editable, zoomControl, zoom, center, scaleMetric, tileServer, osrmServer);
                break;
            default:
                maps = new Map(editable, zoomControl, zoom, center, scaleMetric, tileServer, osrmServer);
        }
    }

    // Скриншот изображения карты
function makeScreenShot() {
