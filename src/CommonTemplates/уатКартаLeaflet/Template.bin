                    maps.selectedLayer.enableEdit()
                } else {
                    // Рисуем новые полигоны
                    maps.selectedLayer = maps.drawPolygon([{
                        "latlng": coordinate.latLngs,
                        color: e.layer._path.attributes['fill'].value,
                        guid: ""
                    }], true);
                    // Включаем режим редактирования
                    maps.selectedLayer.enableEdit();
                }
