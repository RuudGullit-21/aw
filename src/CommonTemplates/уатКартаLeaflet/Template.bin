    }
	
	
    // Функция вызова для 1С 14 версии
    function evalJSFunc(string) {
        eval(string);
    }

    class Map {
        constructor(editable, zoomControl, zoom, center, scaleMetric, tileServer, osrmServer) {
            this.editable = editable || false;
            this.zoom = zoom || 10;
            this.center = center || [55.742793, 37.615401];
            this.zoomControl = zoomControl || false;
            this.scaleMetric = scaleMetric || true;
            this.map = null;
            this.consoleMessage = null;
            this.markerClusterGroup = null;
            this.editableLayers = null;
            this.tileServer = tileServer || `https://{s}.tile.openstreetmap.org/`;
            this.osrmServer = osrmServer || `http://router.project-osrm.org/route/v1/driving/`;
            this.waypoints = [];
            this.totalTime = 0;
            this.totalDistance = 0;
            this.geometry = 0;
            this.initialization();
        }

        initialization() {
            // Проверка была ли уже инициализация карты
            if (!this.map) {
                this.map = L.map("map", {
                    editable: this.editable,            // Возможность редактирования слоев
                    center: this.center,                // Координаты инициализации карты
                    zoom: this.zoom,                    // Установка значения ZOOM
                    zoomControl: this.zoomControl,      // Кнопки zoom(+ / -)
                    doubleClickZoom: false,
                    minZoom: 1,
                });

                // Задаем описание для карты
                L.tileLayer(`${this.tileServer}{z}/{x}/{y}.png`, {attribution: '&copy; <a href="https://rarus.ru" title="1С-Rarus">1C-RARUS</a>'}).addTo(this.map);

                // Добавляем метрическую шкалу
                L.control.scale({metric: this.scaleMetric, imperial: false}).addTo(this.map);

                L.interactivePathRuler(this.map, {
                    rulerLineColor: 'grey',
                    rulerLineWeight: 3,
                    decimalSeparator: '.',
                    unitNameMeter: 'm',
                    unitNameKilometer: 'km',
                    buttonIconOff: '<img alt="" width="16px" src="data:image/svg+xml;base64,">',
                    buttonIconOn: '<img alt="" width="16px" src="data:image/svg+xml;base64,">',
                    buttonTitleOff: 'Измерение расстояний на карте',
                    buttonTitleOn: 'Измерение расстояний на карте',
                    buttonBackgroundColorOff: 'white',
                    buttonBackgroundColorOn: '#ffff00b3',
                });

                // Клик по карте
                this.map.on("click", (e) => {
                    this.reporting("mapClick", "0", "Click on map", "<lat>" + e.latlng.lat + "</lat><lng>" + e.latlng.lng + "</lng>");
                });

                // Даблклик по карте
                this.map.on("dblclick", (e) => {
                    this.reporting("mapDblClick", "0", "dblClick on map", "<lat>" + e.latlng.lat + "</lat><lng>" + e.latlng.lng + "</lng>");
                });

                // Событие которое говорит 1С, новая карта инициализирована
                this.reporting("mapInitialization", "0", "Map was initialization", "");
            }

            // Создаем слой кластеров
            this.markerClusterGroup = L.markerClusterGroup({
                showCoverageOnHover: false,
                zoomToBoundsOnClick: true,
                spiderfyOnMaxZoom: true,
                maxClusterRadius: 140,
                singleMarkerMode: false
            });

            // Создаем общий слой, для добавления, удаления объектов на карте
            this.editableLayers = new L.FeatureGroup().addTo(this.map);
        }

        // Функция инициализации событий на стороне 1С
        reporting(command, code, description, data) {
            this.consoleMessage = "<leafletResponse><command>" + command + "</command><code>" + code + "</code><description>" + description + "</description><data>" + data + "</data></leafletResponse>";
            window.consoleMessage = this.consoleMessage;
			document.getElementById("messageButton").value = this.consoleMessage;
            document.getElementById("messageButton").click();
            console.log(this.consoleMessage);
        }

        // Центрирование по  заданым координатам
        setBoundsToWay(position) {
            this.map.fitBounds(position);
            this.reporting("setBoundsToWay", "0", "Map was setBoundsToWay by coordinate [" + position + "]", "");
        }

        // Заготовки иконок из 1С
        blankIcons(value) {
            switch (value) {
                case 1:
                    blankIcons = L.icon({iconUrl: "<!-- %VehicleIconOnLineFree% -->", iconSize: [32, 32]});
                    break;
                case 2:
                    blankIcons = L.icon({iconUrl: "<!-- %VehicleIconOnLineBusy% -->", iconSize: [32, 32]});
                    break;
                case 3:
                    blankIcons = L.icon({iconUrl: "<!-- %VehicleIconOffLine% -->", iconSize: [32, 32]});
                    break;
                case 4:
                    blankIcons = L.icon({iconUrl: "<!-- %DeparturePointIcon% -->", iconSize: [32, 32]});
                    break;
                case 5:
                    blankIcons = L.icon({iconUrl: "<!-- %DestinationPointIcon% -->", iconSize: [32, 32]});
                    break;
                case 6:
                    blankIcons = L.icon({iconUrl: "<!-- %IntermediatePointIcon% -->", iconSize: [32, 32]});
                    break;
                case 7:
                    blankIcons = L.icon({iconUrl: "<!-- %AddressIcon% -->", iconSize: [32, 32], iconAnchor: [9, 30]});
                    break;
                case 8:
                    blankIcons = L.icon({iconUrl: "<!-- %ZonePointIcon% -->", iconSize: [16, 16]});
                    break;
				case 9:
                    blankIcons = L.icon({iconUrl: "<!-- %VehicleIconPlayRoute% -->", iconSize: [32, 32]});
                    break;
                default:
                    blankIcons = L.icon({
                        iconUrl: "data:image/PNG;base64,",
                        iconSize: [25, 41],
                        iconAnchor: [12, 41]
                    });
            }

            return blankIcons;
        }

        // Создание маркера
        addPointIcon(lat, lon, description = false, markerNum = 0, cluster = false, mark = null, guid = "") {
            // Создаем маркер
            /**
             * coordinate - координаты точки
             * description - описание для всплывашки
             * cluster - признак нужно ли точку отображать в кластере
             */
            let marker = L.marker([lat, lon], {
                guid: guid,
                icon: (mark !== null && mark !== "") ? L.icon({
                    iconUrl: mark,
                    iconSize: [32, 32]
                }) : this.blankIcons(markerNum)
            });

            // Проверяем есть ли у него описание
            description ? marker.bindPopup(description.toString()) : marker;

            // Удаляем обработчик со всех меток, чтобы отловить событие по dbclick
            marker.off("click");

            // На основании параметра распределем какие маркеры пойдут в кластер, а какие нет
            cluster ? this.editableLayers.addLayer(this.markerClusterGroup.addLayer(marker)) : this.editableLayers.addLayer(marker);

            // Навешиваем обработчик для отслеживания клика по точке
            let timeoutId;
            marker.on("click", (e) => {
                timeoutId = setTimeout(() => {
                    marker.openPopup();
                    this.reporting("pointClick", "0", "Point click", "<lat>" + marker.getLatLng().lat + "</lat><lon>" + marker.getLatLng().lng + "</lon>");
                }, 300);
            });

            // Навешиваем обработчик для отслеживания id по точке
            // Навешиваем обработчик для отслеживания id по точке
            marker.on("dblclick", () => {
                clearTimeout(timeoutId);
                clearTimeout(timeoutId - 1);
                this.reporting("pointClick", "0", "Point click id", marker.options.guid);
            });

        }

        // Работа с навигацией
        togglePanel(_show, _name) {
            let _el = document.querySelector(_name);
            _show ? _el.style.display = 'block' : _el.style.display = 'none';
        }

        // Функция возвращения отформатированых координат для leaflet и 1С
        coordinateFormat(e) {
            let latLngs = e.getLatLngs()[0].map(function (latLngs) {
                return [latLngs.lat, latLngs.lng];
            });
            let latLngs1C = e.getLatLngs()[0].map(function (latLngs) {
                return ['<lat>' + latLngs.lat + '</lat><lon>' + latLngs.lng + '</lon>'];
            });
            return {
                latLngs: latLngs,
                latLngs1C: latLngs1C
            }
        }

        // Функция удаления объектов по guid
        deleteObject(guid) {
            //todo подумать, как сделать более красиво
            this.editableLayers.eachLayer((e) => {
                if (e.options.guid === guid) {
                    this.editableLayers.removeLayer(e);
                }
            });

            // Проверяем есть ли у нас в кластере что-то, чтобы не перебирать массив
            if (this.markerClusterGroup.getLayers().length) {
                this.markerClusterGroup.eachLayer((e) => {
                    if (e.options.guid === guid) {
                        this.markerClusterGroup.removeLayer(e);
                    }
                });
            }
        }

        // Декодирование координат
        decodeString(e, o) {
            let r = e.length, t = 0, n = 0, i = 0;
            for (o = Math.pow(10, -o); t < r;) {
                for (var l, c = 0, d = 0; d |= (31 & (l = e.charCodeAt(t++) - 63)) << c, c += 5, 32 <= l;) ;
                for (n += 1 & d ? ~(d >> 1) : d >> 1, d = c = 0; d |= (31 & (l = e.charCodeAt(t++) - 63)) << c, c += 5, 32 <= l;) ;
                i += 1 & d ? ~(d >> 1) : d >> 1, this.waypoints.push([n * o, i * o])
            }
        }

        // Получаем координаты OSR
        getTraceWay(coordinate, ParseString) {
            let response;
            let OSR;

            if (ParseString === "") {
                // Сервис обработки OSR
                OSR = `${this.osrmServer}${coordinate[0][1]},${coordinate[0][0]};${coordinate[1][1]},${coordinate[1][0]}?overview=full` || `http://router.project-osrm.org/route/v1/driving/${coordinate[0][1]},${coordinate[0][0]};${coordinate[1][1]},${coordinate[1][0]}?overview=full`;

                // Cоздаем XHR запрос
                var xhr;

                try {
                    xhr = new XMLHttpRequest;
                } catch (e) {
                    this.reporting("getTraceWay", "1", "Error: " + e, "");
                    return [];
                }

                // Отправляем GET к сервису OSR в специальном формате
                xhr.open("GET", OSR, false);
                try {
                    xhr.send()
                } catch (e) {
                    this.reporting("getTraceWay", "1", "Error: " + e, "");
                    return [];
                }
                response = JSON.parse(xhr.responseText);
            } else {
                response = JSON.parse(ParseString);
            }

            if (response.code !== "Ok") {
				if (response.code == "OK_Яндекс") {
					// Сервис отработа корректно, вернул несколько точек, время, дистанцию
					this.totalTime += response.totalTime;
					this.totalDistance += response.totalDistance;
					 for (let i = 0; i < response.points.length; i++) {
						this.waypoints.push([response.points[i][0], response.points[i][1]]);
					}
					this.reporting("getTraceWay", "0", "Route found", "");
					return this.waypoints;
				} else {
					// Сервис вернул ошибку
					this.reporting("getTraceWay", xhr.status, "Service OSRM ERROR", response);
					return [];
				}
            } else if (response['routes'].length === 0) {
                // Сервис отработал корректно, но координат нет
                this.reporting("getTraceWay", "2", "No route", "");
                return [];	
            } else {
                // Сервис отработа корректно, вернул несколько точек, время, дистанцию
                this.totalTime += response.routes[0].duration;
                this.totalDistance += response.routes[0].distance;
                this.geometry = response.routes[0].geometry;
                this.decodeString(this.geometry, 5);
                this.reporting("getTraceWay", "0", "Route found", "");
                return this.waypoints;
            }
        }

        // Рисуем кастомную линию
        showPolyline(coordinate, guid = "", color = "blue", weight = 5, arrowHeard = true, dashArray = '0') {
            let layer = L.polyline(coordinate, {color: color, opacity: .7, weight: weight, guid: guid, dashArray: dashArray, dashOffset: '0'});
            let layerDecorator = L.polylineDecorator(coordinate, {
                guid: guid,
                patterns: [{
                    offset: 10,
                    repeat: 100,
                    symbol: new L.Symbol.arrowHead({
                        pixelSize: 6,
                        polygon: !0,
                        pathOptions: {weight: weight, color: color, stroke: !0, lineJoin: "miter"},
                    })
                }]
            });

            this.editableLayers.addLayer(layer);

            if (arrowHeard) {
                this.editableLayers.addLayer(layerDecorator);
            }
        }

        // Проложить путь по координатам
        traceRoute(coordinate, guid, fact, plan, color, weight, ParseString = "") {
            this.waypoints = [];
            this.totalTime = 0;
            this.totalDistance = 0;
            this.getTraceWay(coordinate, ParseString);
            if (this.waypoints.length > 0) {
                this.showPolyline(this.waypoints, guid, color, weight);
				if (plan) {
					this.reporting("traceRoute", "0", "Trace complete", "<totalTime>" + this.totalTime + "</totalTime><totalDistance>" + this.totalDistance + "</totalDistance><plan>true</plan><waypoints>" + this.waypoints + "</waypoints>");
				} else if (fact) {
					this.reporting("traceRoute", "0", "Trace complete", "<totalTime>" + this.totalTime + "</totalTime><totalDistance>" + this.totalDistance + "</totalDistance><fact>true</fact><waypoints>" + this.waypoints + "</waypoints>");
				} 
				else {
					this.reporting("traceRoute", "0", "Trace complete", "<totalTime>" + this.totalTime + "</totalTime><totalDistance>" + this.totalDistance + "</totalDistance>");
				}
            } else {
                this.reporting("traceRoute", "1", "No way", "<totalTime>0</totalTime><totalDistance>0</totalDistance>");
            }
        }

        // Выбор геозоны
        geoSelection(e) {
            if (maps.unSelectedLayer !== e.target) {
                // Проверяем существует ли предыдущая область
                if (maps.unSelectedLayer == null && maps.selectedLayer != null) {
                    // удаляем слой который не сохранили
                    // this.maps.removeLayer(maps.selectedLayer);

                    // очищаем рабочие слои
                    maps.unSelectedLayer = maps.selectedLayer = null;

                    // скрываем кнопки управления
                    maps.togglePanel(false, '#polygonSaveButton');
                    maps.togglePanel(false, '#polygonCancelButton');
                }
                if (maps.unSelectedLayer != null) {
                    // отключаем редактирование области
                    maps.unSelectedLayer.disableEdit();

                    // восстанавливаем его старые координаты
                    maps.unSelectedLayer.setLatLngs(maps.oldLatLngs);
                }
                // Обновляем объект по которому произошел клик
                maps.unSelectedLayer = maps.selectedLayer = e.target;

                if (maps.selectedLayer != null) {
                    maps.togglePanel(true, '.action-update');
                }

