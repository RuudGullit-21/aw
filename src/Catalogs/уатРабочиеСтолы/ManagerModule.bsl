// Выполняет заполнение предопределенных элементов справочника
//
Процедура ЗаполнитьПредопределенныеДанные() Экспорт
	
	ТекЭлемент = Справочники.уатРабочиеСтолы.ЗаказыМаршрутныеЛисты;
	Панели = Новый Структура("Панель1, Панель2, Панель3, Панель4");
	Панели.Панель1 = "Заказы";
	Панели.Панель3 = "МаршрутныеЛисты";
	ЗаполнитьРабочиеСтолы(ТекЭлемент, НСтр("en = 'AWP Logistician'; ru = 'АРМ Логиста'"), Панели);
	
	ТекЭлемент = Справочники.уатРабочиеСтолы.ЗаказыСтатистикаМаршрутныеЛистыПланировщик;
	Панели = Новый Структура("Панель1, Панель2, Панель3, Панель4");
	Панели.Панель1 = "Заказы";
	Панели.Панель2 = "Статистика";
	Панели.Панель3 = "МаршрутныеЛисты";
	Панели.Панель4 = "Планировщик";
	ЗаполнитьРабочиеСтолы(ТекЭлемент, НСтр("en = 'AWP Logistician'; ru = 'АРМ Логиста'"), Панели);
	
	ТекЭлемент = Справочники.уатРабочиеСтолы.Карта;
	Панели = Новый Структура("Панель1, Панель2, Панель3, Панель4");
	Панели.Панель1 = "Карта";
	ЗаполнитьРабочиеСтолы(ТекЭлемент, НСтр("en = 'AWP Transportation control'; ru = 'АРМ Контроль перевозок'"), Панели);
	
	ТекЭлемент = Справочники.уатРабочиеСтолы.КартаПункты;
	Панели = Новый Структура("Панель1, Панель2, Панель3, Панель4");
	Панели.Панель1 = "Карта";
	Панели.Панель3 = "Пункты";
	ЗаполнитьРабочиеСтолы(ТекЭлемент, НСтр("en = 'AWP Transportation control'; ru = 'АРМ Контроль перевозок'"), Панели);

	ТекЭлемент = Справочники.уатРабочиеСтолы.КартаСобытия;
	Панели = Новый Структура("Панель1, Панель2, Панель3, Панель4");
	Панели.Панель1 = "Карта";
	Панели.Панель3 = "События";
	ЗаполнитьРабочиеСтолы(ТекЭлемент, НСтр("en = 'AWP Transportation control'; ru = 'АРМ Контроль перевозок'"), Панели);
	
	ТекЭлемент = Справочники.уатРабочиеСтолы.НайтиПоНаименованию("Карта + Отчет ""Анализ план факта по Заказам""",Истина);
	Если ТекЭлемент.ПометкаУдаления Тогда
		СпрОбъект = ТекЭлемент.ПолучитьОбъект();
		СпрОбъект.Удалить();
	КонецЕсли;
	
КонецПроцедуры

#КонецОбласти

#Область СлужебныеПроцедурыИФункции

// Заполняет поля элемента справочника "уатРабочиеСтолы"
//
Процедура ЗаполнитьРабочиеСтолы(РабочийСтол, Знач Назначение, Знач Панели)
	
	СпрОбъект = РабочийСтол.ПолучитьОбъект();
	СпрОбъект.Назначение = Назначение;
	СпрОбъект.НастройкиПанелей.Очистить();
	Для Счетчик = 1 По 4 Цикл
		НоваяСтрока = СпрОбъект.НастройкиПанелей.Добавить();
		НаименованиеПанели = "Панель" + Счетчик;
		НоваяСтрока.Панель   = НаименованиеПанели;
		Если Панели.Свойство(НаименованиеПанели) И ЗначениеЗаполнено(Панели[НаименованиеПанели]) Тогда
			Если ТипЗнч(Панели[НаименованиеПанели]) = Тип("Структура") Тогда
				НоваяСтрока.Значение      = Панели[НаименованиеПанели].Значение;
				НоваяСтрока.СсылкаНаОтчет = Панели[НаименованиеПанели].СсылкаНаОтчет;
			Иначе
				НоваяСтрока.Значение = Панели[НаименованиеПанели];
			КонецЕсли;
		КонецЕсли;
	КонецЦикла;
	СпрОбъект.Записать();
	
КонецПроцедуры

#КонецОбласти

#КонецЕсли